<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>EquiposControlador</servlet-name>
        <servlet-class>servlets.EquiposControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstadiosControlador</servlet-name>
        <servlet-class>servlets.EstadiosControlador</servlet-class>
        <multipart-config></multipart-config>
    </servlet>
    <servlet-mapping>
        <servlet-name>EquiposControlador</servlet-name>
        <url-pattern>/ListarEquipos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EquiposControlador</servlet-name>
        <url-pattern>/Equipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstadiosControlador</servlet-name>
        <url-pattern>/Estadio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstadiosControlador</servlet-name>
        <url-pattern>/ListarEstadios</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
